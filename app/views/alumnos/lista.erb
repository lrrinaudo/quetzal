<h1>Lista de Alumnos</h1>

<table class="table table-bordered">
	<tr>
		<th> Nombre </th>
		<th> Apellido </th>
		<th> Email </th>
		<th> Cantidad de Resumenes </th>
		
	</tr>
	<% @alumnos.each do | alumno | %>

	<tr>
		<td>
			<%= alumno.nombre %>
		</td>
		<td>
			<%= alumno.apellido %>
		</td>
		<td>
			<%= alumno.email %>
		</td>
		<td>
			<%= alumno.cant_resumenes %>
		</td>		
	</tr>

  <% end %>

</table>
